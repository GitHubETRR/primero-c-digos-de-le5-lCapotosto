#include <stdio.h>
int suma(int vec[5]);

int main() {
    int vec[5] = {31, 10, 10, 10, 10};
    int resultado = suma(vec);
    printf("El resultado de la suma es %d", resultado);
}

int suma(int vec[5]) {
    int suma = 0;
    for (int i = 0; i < 5; i++) {
        suma += vec[i];
    }
    return suma;
}
